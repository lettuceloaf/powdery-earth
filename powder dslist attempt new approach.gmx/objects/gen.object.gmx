draw_text(view_xview[0]+16,view_yview[0]+48,"Roughness: "+string(roughness))

/*
draw_set_color(c_red)
draw_text(view_xview[0]+16,view_yview[0]+16,"P-Min: "+string(pmin))
draw_line(0,room_height-(pmin*BLOCK_WIDTH),window_get_width(),room_height-(pmin*BLOCK_WIDTH))
draw_set_color(c_green)
draw_text(view_xview[0]+16,view_yview[0]+32,"P-Max: "+string(pmax))
draw_line(0,room_height-(pmax*BLOCK_WIDTH),window_get_width(),room_height-(pmax*BLOCK_WIDTH))
*/

draw_set_alpha(0.2)
draw_set_color(c_maroon)
draw_rectangle(0,(room_height-(pmin*BLOCK_WIDTH))-(pmin_delta*BLOCK_WIDTH),window_get_width(),(room_height-(pmin*BLOCK_WIDTH))+(pmin_delta*BLOCK_WIDTH),0)
draw_set_color(c_lime)
draw_rectangle(0,(room_height-(pmax*BLOCK_WIDTH))-(pmax_delta*BLOCK_WIDTH),window_get_width(),(room_height-(pmax*BLOCK_WIDTH))+(pmax_delta*BLOCK_WIDTH),0)

if keyboard_check(ord("G"))
{
draw_set_alpha(0.35)
draw_set_color(c_gray)
var gx,gy;
for (gx=0;gx&lt;room_width;gx+=BLOCK_WIDTH*2) {
draw_line(gx,0,gx,room_height)
}
for (gy=0;gy&lt;room_height;gy+=BLOCK_WIDTH*2) {
draw_line(0,gy,room_width,gy)
}
    if keyboard_check(ord("T")) then
    {
    draw_set_alpha(0.45)
    draw_set_color(c_white)
    var zx,zy;
    for (zx=0;zx&lt;ds_grid_width(mgrid);zx+=1) {
    draw_text(gx*BLOCK_WIDTH,gy*BLOCK_WIDTH,string(ds_grid_get(mgrid,gx,gy)))
    }
    for (zy=0;zy&lt;ds_grid_height(mgrid);zy+=1) {
    draw_text(gx*BLOCK_WIDTH,gy*BLOCK_WIDTH,string(ds_grid_get(mgrid,gx,gy)))
    }   
    }
}


draw_set_alpha(1)
draw_set_color(c_white)
}


</string>
